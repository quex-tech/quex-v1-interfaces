{
  "name": "quex-v1-interfaces",
  "version": "1.0.0",
  "description": "Quex interfaces",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quex-tech/quex-v1-interfaces.git"
  },
